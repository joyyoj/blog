#releaseProtoDetailDialog.modal {
    width: 900px;
    margin-left: -450px;
}

div.leftNavPanel {
    width: 22%;
    overflow: hidden;
    margin-left: 10px;
    float: left;
}

div.rightPanel {
    float: right;
    position: relative;
    border: 1px solid #ddd;
    width: 73%;
    margin: 0 10px 10px 0;
}

div.rightPanel .sample{
    z-index: 999;
    position: absolute;
    float: left;
}

div.btnRight {
    padding: 5px 0px 5px 0px;
}

#slideTreeBtnBox {
    background: #f3f3f3;
    width: 13px;
    float: left;
    cursor: pointer;
}

    
#info-left, #info-right {
    width: 46%;
    margin-left: 10px;
}

#info-right.span6 {
    margin-left: 30px;
}

.logInfobtnGroup {
    padding: 0 10px;
    margin-bottom: 10px;
}

#slideTreeBtn {
    cursor: pointer;
    position: relative;
    top: 10%;
    left: 0px;
}

#queryTextarea {
    width: 99%;
}
.CodeMirror {
    height: 250px;
    background: #fff;
    font-size: 14px;
    border: 1px solid #6DA5DB;
}
.CodeMirror-scroll {
    height: 250px;
}

#dialog {
    background: #fff;
    position: absolute;
    margin: auto;
    z-index:100000000;
    width: 400px;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-box-shadow: 0 0 10px #fff;
    box-shadow: 0 0 15px #fff;
    padding: 15px;
    border: 1px solid #b3b3b3;
}


.dialog-header h4 {
    display: block;
    height: 30px;
    line-height: 30px;
    margin: 0 auto;
    font-size: 14px;
    text-align: center;
    color: #333;
    border-bottom: 1px solid #e3e3e3;
}

.dialog-body {
    margin: 20px 0 20px 0;
}

.dialog-body p {
    border: 1px solid #e3e3e3;
    background: #DA1074;
    line-height: 40px;
    font-size: 20px;
    font-weight: bold;
    text-align:center; 
    margin: 0px 0px;
    color: #fff;
}

.dialog-footer {
    text-align: center;
}

/* *** *** --- 排队队列 Start  --- *** *** */

div.ready-queue{
    margin: 10px 0;
}

div.ready-queue li{
    list-style-type: none;
}

div.ready-queue dl{
    padding: 0px 15px;
    margin: 0;
}

div.ready-queue dl:hover{
    background: #f3f3f3;
}

div.ready-queue dl dt{
    display: inline-block;
    line-height: 40px;
}

div.ready-queue dl dd{
    display: inline-block;
    line-height: 40px;
}

div.ready-queue dl dd.ready-queue-operator{
    float: right;
    margin-left: 5px;
}


div.ready-queue dl dd.ready-queue-operator:hover{
    background: #fff;
}

div.ready-queue dl dd.ready-queue-operator a{
    margin-top: 15px;
}

/* *** *** --- 排队队列 End --- *** *** */


/* *** *** --- 执行队列 Start --- *** *** */
div.running-queue{
    margin: 10px 0;
    padding: 0px;
}

div.running-queue li{
    border: 1px solid #e3e3e3;
    margin-bottom: 10px;
    list-style-type: none;
}

div.running-queue li dl{
    margin: 0px;
}

div.running-queue li dl dd{
    display: inline-block;
}

div.running-queue li dl.running-info{
    height: 30px;
    line-height: 30px;
    background: #f9f9f9;
}

div.running-queue li dl.running-info dt{
    float: right;
    margin: 7px 15px 0 0;
}

div.running-queue li dl.running-info dd{
    margin-top: 5px;
}

div.running-queue li dl.running-info dd.kill-running{
    float: right;
    margin: 7px 20px 0 0;
}

div.running-queue li dl.running-info dd span{
    background: #fff;
    padding: 0 5px 0 0px;
    border: 1px solid #e3e3e3;

}

div.running-queue li dl.running-progress dd{
    display: block;
    height: 15px;
    width: 95%;
    margin: 10px auto;
    border: 1px solid #e3e3e3;
    border-radius: 8px;
    background: #f9f9f9;
}

div.running-queue li dl.running-progress dt{
    display: block;
    height: 40px;
    padding: 10px;
}

div.running-queue li dl.running-progress dt.progress-monitor span{
    display: block;
    height: 40px;
    width: 90%;
    line-height: 40px;
    margin: 0 auto;
    border: 1px solid #e3e3e3;
    text-align: center;
    background: #fff;
}

/* *** *** --- 执行队列 End --- *** *** */

/* *** *** --- 历史队列 Start --- *** *** */
