@chartset utf-8;
.clearfix{
    clear:both;
}
*,body,dl,dt,dd{
    padding:0px;
    margin:0px;
}
#add_queue_panel{
    padding:15px;
    background:#fefefe;
    border:1px solid #e3e3e3;
    border-radius:5px;
    -moz-border-radius:5px;
    -webkit-border-radius:5px;
    Box-shadow:0px 0px 5px #d3d3d3;
    -moz-shadow:0px 0px 5px #d3d3d3;
    -webkit-shadow:0px 0px 5px #d3d3d3;
}
div#add_queue_panel{
    padding:30px 50px;
}

div#add_queue_panel form{
    margin-top:40px;
}


div#add_queue_panel h4{
    display:inline-block;
    float:left;
    height:20px;
    line-height:20px;
    font-size:12px;
    font-weight:bold;
    color:#333;
    Box-shadow:0px 0px 5px #d3d3d3;
    -moz-box-shadow:0px 0px 5px #d3d3d3;
    -webkit-border-shadow:0px 0px 5px #d3d3d3;
    padding:5px 10px;
    width:80px;
}

div#add_queue_panel span.line-cross{
    display:block;
    margin-left:100px;
    margin-right:10px;
    border-bottom:1px solid #e3e3e3;
    height:13px;
    line-height:13px;
}

dl{
    margin-top:5px;
    padding-bottom:5px;
    width:450px;
}

dl dt{
    float:left;
    width:65px;
    height:30px;
    line-height:30px;
    color:#333;
    font-weight:normal;
}
dl dd{
    position:relative;
}
div#add_queue_panel dl dd input{
    color:#d3d3d3;
}

dl.radio_wrap{
}

dl.radio_wrap span{
    width:218px;
    border:1px solid #e3e3e3;
    border-radius:3px;
    -moz-border-radius:3px;
    -webkit-border-radius:3px;
    display:inline-block;
}
dl.radio_wrap dd{
    display:inline-block;
    height:30px;
    line-height:30px;
    width:102px;
}

dl.radio_wrap dd input{
    display:inline-block;
    margin-left:35px;
    height:25px;
}

dl.radio_wrap dd label{
    display:inline-block;
}

dl.radio_wrap dd.active {
}

dl#user_wrap{
    clear:both;
}

dl#pub_panel{
    clear:both;
}

dl#group,dl#user,dl#queue,dl#pass{
   //display:inline-block;
   width:450px;
    //display:block;
}

dl#group,dl#user{
    float:left;
}

dl#queue,dl#pass{
    margin-left:10px;
    display:block;
    float:right;
}

dl#group dt,dl#queue dt,dl#user dt,dl#pass dt {
   //display:inline-block;
}

dl#group dd,dl#queue dd,dl#user dd,dl#pass dd {
   //display:inline-block;
}

dl#queueresource{
    clear:both;
}

div#notic{
    width:50px;
    height:30px;
    background:#fd8888;
    border:1px dashed #e3e3e3;
    border-radius:3px;
    -moz-border-radius:3px;
    -webkit-border-radius:3px;
    line-height:30px;
    padding:0 10px;
}
.icon-white{
    z-index:999;
}
.icon-remove{
    z-index:999;
}
div#notic i.error-notic{
}

