@chartset utf-8;
.clearfix{
    clear:both;
}
*,body,dl,dt,dd{
    padding:0px;
    margin:0px;
}
h1,h2,h3,h4,h5,h6{
    font-size:20px;
    height:38px;
    line-height:38px;
    font-weight:normal;
    border-bottom:1px solid #e3e3e3;
    margin-bottom:25px;
}
#add_queue_panel{
    padding:30px 50px;
    background:#fefefe;                                                   
    border:1px solid #e3e3e3;                                                      
    border-radius:5px;                                                             
    -moz-border-radius:5px;                                                        
    -webkit-border-radius:5px;                                                     
    Box-shadow:0px 0px 5px #d3d3d3;                                                
    -moz-shadow:0px 0px 5px #d3d3d3;                                               
    -webkit-shadow:0px 0px 5px #d3d3d3;
}
div#add_queue_panel h4{
    display:inline-block;                                                          
    float:left;                                                                    
    height:20px;                                                                   
    line-height:20px;                                                              
    font-size:12px;                                                                
    font-weight:bold;                                                              
    color:#333;                                                                    
    Box-shadow:0px 0px 5px #d3d3d3;                                                
    -moz-box-shadow:0px 0px 5px #d3d3d3;                                           
    -webkit-border-shadow:0px 0px 5px #d3d3d3;                                     
    padding:5px 10px;                                                              
    width:80px;                                                                    
}

div#add_queue_panel span.line-cross{                                               
    display:block;                                                                 
    margin-left:100px;                                                             
    margin-right:10px;                                                             
    border-bottom:1px solid #e3e3e3;                                               
    height:13px;                                                                   
    line-height:13px;                                                              
} 

dl{
    height:40px;
    line-height:40px;
    margin-top:5px;
    padding-bottom:5px;
    //width:330px;
    clear:both;
}

dl dt{
    float:left;
    width:65px;
    height:30px;
    line-height:30px;
    color:#333;
    font-weight:normal;
}

div#add_queue_panel dl dd.yes{
    background:#e3e3e3;
    display:inline-block;
    height:30px;
    line-height:30px;
    width:40px;
    border-top-left-radius:3px;
    border-bottom-left-radius:3px;
}

div#add_queue_panel dl dd.no{
    display:inline-block;
    height:30px;
    line-height:30px;
    width:40px;
    border-top-right-radius:3px;
    border-bottom-right-radius:3px;
    color:#e3e3e3;
}

div#add_queue_panel dl dd font{
    text-align:center;
    display:block;
}

div#add_queue_panel dl dd span{
    display:inline-block;
    width:300px;
    height:20px;
    text-indent:5px;
    border-bottom:1px solid #e3e3e3;
    color:#333;
}
dl.radio_wrap span{
    border:1px solid #e3e3e3;
    border-radius:3px;
    -moz-border-radius:3px;
    -webkit-border-radius:3px;
    display:inline-block;
}
dl.radio_wrap dd{
    float:left;
    display:block;
    padding:0 32px;
}

dl.radio_wrap dd input{
    display:inline-block;
}

dl.radio_wrap dd label{
    display:inline-block;
}

dl.radio_wrap dd.yes {
    //float:left;
}

dl#user_wrap{
    clear:both;
}

dl#pub_panel{
    clear:both;
}

div#common{
    margin-top:20px;
}

div#common label{
    float:left;
}

div#common span{
    display:block;
    padding-left:67px;
}

div#common span p{
    border-bottom:1px solid #e3e3e3;
    padding-bottom:3px;
    padding-right:50px;
    display:inline-block;
}

dl#group,dl#user,dl#queue,dl#pass{
   //display:inline-block;
}

dl#group dt,dl#queue dt,dl#user dt,dl#pass dt {
   //display:inline-block;
}

dl#group dd,dl#queue dd,dl#user dd,dl#pass dd {
   //display:inline-block;
}




