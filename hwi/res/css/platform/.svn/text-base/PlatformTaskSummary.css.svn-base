 div.history, div.runtime{
    font-size:14px;
    color:#000;
 }
 /* 运行时信息 */                                                            
 div.runtime{                                                                
     padding:0px 0px 20px 0px;                                                 
 }
 div.runtime div label{                                                                
    display:inline-block;
    margin:10px 10px;
    font-size:14px;
 }

 div.runtime div label input{
    margin-right:5px;
 }

 table{                                                                      
     width:100%;                                                               
     border-collapse:collapse;                                                 
 }                                                                           
 table th{                                                                   
     border:1px solid #E3E3E3;                                                 
 }                                                                           
 table td{                                                                   
     border:1px solid #E3E3E3;                                                 
 }                                                                           
 table td.head{                                                              
     width:100px;                                                              
     font-weight:bold;                                                         
     text-align:center;                                                        
 }

 table tbody tr{
    height:25px;
 }

 table tbody tr td{
    padding-left:5px;
 }
 .errorTip{                                                                  
     color:red;                                                                
 }                                                                           
                                                                                                                                                         
 /* 历史信息 */                                                              
 div.history{                                                                
     padding:10px 0px 20px 0px;                                                
 }                                                                           
 div.history div{
    margin: 10px; 10px;
 }

 img.chart{                                                                  
        border:1px solid #E3E3E3;                                                
        cursor:pointer;                                                          
 }                                                                           
 img.selected{                                                               
        border:1px solid blue;                                                   
        cursor:pointer;                                                          
 }                                                                           
                                                                                                                                                         
 .title{                                                                     
    padding:5px 5px 5px 5px;                                                
    background-color:#0088CC;                                               
    color:white;                                                            
 }

 h3.title a{
    color:white;                                                            
 }
