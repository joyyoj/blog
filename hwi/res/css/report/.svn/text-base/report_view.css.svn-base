@chartset utf-8;
body{
    font-ize: 62.5%;
    font-family: "Trebuchet MS", "Arial", "Helvetica", "Verdana", "sans-serif";
}

dl.report-page-top{
    border-bottom:1px #e3e3e3 solid;                                            
    max-width:1170px;
    height:30px;
    margin:0 auto;
    color:#333;
    font-size:14px;
    padding:5px 10px 5px 15px;
}

dt.content-title{
    float:left;
}

dt.content-title h4{                                                               
    height:30px;
    line-height:30px;
}                                                                               

dd.back-previus-page{
    float:right;
   // margin-top:5px;
   // margin-right:10px;
    
}

.wrap-box{                                                                      
    //border:1px #e3e3e3 solid;                                                    
    //margin-top:10px;  
    clear:both;
    margin:0px;
    padding:0px;
}                                                                               

ul.report-view-control-panel{                                     
    list-style-type:none;                                                       
    margin:0 0 10px 0;
    padding:0 10px 10px 0;
    height:20px;
}                                                                               

ul.report-view-control-panel li{                                     
    float:left;
    margin-right:20px;
}

li#count_iterm{
    position:relative;
}

#count_iterm_btn{

}

ul.dropdown-iterm{
    list-style-type:none;
    padding:3px;
    background:white;
    border:1px solid #e3e3e3;
    Border-radius:5px;
    -moz-border-radius:5px
    -webki-border-radius:5px;
   // width:100px;
    z-index:9999;
    position:absolute;
    top:30px;
    left:-25px;
}

ul.dropdown-iterm li{
    border-bottom:1px solid #e3e3e3;
    padding:0px;
   // width:80px;
    margin-left:10px;
    height:20px;
    line-height:20px;
}

ul.dropdown-iterm li:hover{
    background:#6DA5DB;
}

ul.dropdown-iterm li span{
    //display:block;
    //width:80px;
    text-align:center;
    cursor:pointer;
}

.click-down-btn{
    height:5px;
}

.recent-date{
    padding-top:7px;
}

.recent-date font{
    cursor:pointer;
}

.date-area{
}

.date-area span font{
}

.date-picker{
    display:inline-block;
    height:25px;
    line-height:25px;
    font-ize: 62.5%;
    font-family: "Trebuchet MS", "Arial", "Helvetica", "Verdana", "sans-serif";
}

.date-picker input{
    margin-top:5px;
}

span.change-chart-btn{
    float:right;
    margin-right:10px;
}

span.change-chart-btn:hover{
}

span.change-chart-btn img{
    cursor:pointer;
}

#report_container{
    
}

.date-base-time{
}

.date-base-time-lable font{
    margin-left:10px;
    height:30px;
    line-height:30px;
    vertical-align:middle;
}

#datepicker_base_time{
    margin-top:10px;
}

#report_table tbody tr td span{
    cursor:pointer;
    display:block;
}

#bar_chart,#line_chart{
}

#bar_chart img,#line_chart img{
    border:1px solid #fff;
    Border-radius:3px;
    -moz-border-radius:3px;
    -webkit-border-radius:3px;
}

#bar_chart img:hover,#line_chart img:hover{
    border:1px solid #e3e3e3;
    Border-radius:3px;
    -moz-border-radius:3px;
    -webkit-border-radius:3px;
}
