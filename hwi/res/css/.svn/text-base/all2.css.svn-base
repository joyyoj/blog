/*
 * BigLog global CSS
 */

*{margin:0; padding:0;}
body {font-family:Arial,sans-serif; font-size:12px; color:#333; }
.l{float:left;}.r{float:right;}.c{clear:both;}.b{font-weight:bold}
.mg{margin:10px auto}.hd{display:none !important}.hl{color:#f88000;font-weight:bold;}
.pd{padding:10px;}
.pds{padding:5px;}
.mgt{margin-top:10px;}.mgb{margin-bottom:10px;}.mgl{margin-left:10px;}.mgr{margin-right:10px;}
.mgt20{margin-top:20px;}
.mgbs{margin-bottom:5px;}
.bd{border:1px solid #ddd}.bdb{border-bottom:1px solid #ddd;}
.bg{background:#F7F7F7;}.red{color:red}
.com{width:1000px; margin:10px auto; }
.center{text-align:center;}
.cp{cursor:pointer;}
input.tiny{ width:45px;}
select.tiny{ width:65px;}
.form-horizontal .controls {margin-left: 115px;}
.inlineblock {display:inline-block; margin-right:20px;}
.form-horizontal .controls p.help-block {color: #0088CC;}
.btn{font-family: "Microsoft Yahei","华文黑体","Arail","Helvetica","sans-serif";}

ul.breadcrumb li.disabled {color:#C9C9C9;}

.fastLinkPanel{display:inline-block; float: right;}
.fastLinkPanel>a.btn-mini{padding: 1px 3px;}

.wrap{margin:10px; }
.boxhtml{margin-bottom:20px; height:100%; overflow:hidden;}
.boxhead{border-bottom:2px solid #6DA5DB; }
.boxtitle{background:#6DA5DB;color:#fff; height:20px;padding:4px 30px; font-size:16px;font-weight:bold;font-family:"Microsoft Yahei","华文黑体","Arail","Helvetica","sans-serif"; }
.boxtitle a{color:#fff; font-weight:bold; font-family:"Microsoft Yahei","华文黑体","Arail","Helvetica","sans-serif"; }
.boxbody{}

.ViewTable{width:100%;border-collapse:collapse;}
.ViewTable td{background:#fff; padding:5px;border:1px solid steelblue;}
.ViewTable td input{width:300px; }
.ViewTable td select{width:302px; }
.ViewTable .bgbl{background:#6DA5DB; color:#fff; width:100pX;}

.dataTable td input{cursor:pointer;}

.msgBoxDiv{position:fixed; z-index:20000; width:auto;padding-top:42px;height:24px;top:2px;text-align:center;left:20%;right:20%; pointer-events:none;}
.msgItem{background:#68AF02;color:white;display:inline-block;z-index:99;border:1px solid transparent;padding:3px 24px 3px;border-radius:3px;font-family:"Helvetica Neue","lucida Grande","Verdana";}
.errmsgItem{background:#EAA000;color:white;display:inline-block;z-index:99;border:1px solid;padding:3px 24px 3px;border-radius:3px;font-family:"Helvetica Neue","lucida Grande","Verdana";}
.loadingmsgItem{background:#4B981D url(../img/ico_loading104474.gif) no-repeat 5px 2px;color:white;display:inline-block;z-index:98;border:1px solid;padding:3px 14px 3px 30px;border-radius:3px;font-family:"Helvetica Neue","lucida Grande","Verdana";}
.checkerrmsgItem{background:#FFF2E9 url('../img/sys_icons.gif') no-repeat 3px 2px;display:inline-block;z-index:99;border:1px solid #F60;padding:2px 7px 3px 22px;margin-left:5px;font-family:"Helvetica Neue","lucida Grande","Verdana";}

#canStartProcess td{padding:5px; vertical-align:middle; }


.DataQuality{width:100%;border-collapse:collapse;}
table.DataQuality tr {background-color:white;}
table.DataQuality tr:hover {background-color:#ECECFF;}
.DataQuality td {border:2px solid white; padding: 5px;}
.DataQuality .tbhd{background:#6DA5DB; color:#fff;}
.DataQuality .tbhdt{background:#6DA5DB; color:#fff;}
.DataQualityEntryGOOD{background:#02A21A;color:white;padding:1px 18px 1px 15px;font-weight:bold;}
.DataQualityEntryNORMAL{background:#EAA000;color:white;padding:1px 7px 1px 10px;font-weight:bold;}
.DataQualityEntryBAD{background:#DC542E;color:white;padding:1px 20px 1px 23px;font-weight:bold;}
.DataQualityEntryNULL{background:gray;color:white;padding:1px 3px 1px 3px;font-weight:bold;}
#productAddPanel table td{padding:5px;}

.treecontainer{background:#fff;}
.eventtreecontainer,.loggingtreeschema {width: 20%; }
.udwschemacontainer,.loggingschemacontainer {width: 78%; }
/*.Filtertable tbody tr td.filteroptions span{ width:104px; padding:4px 0px; text-align:center }
.filtertable thead tr th.filtertitlebar span{ width:96px; padding:4px 0px; text-align:center }*/

.filtertable{
    text-align:left;
    width:100%;
}

.filtertable .filterkey{
    font-weight:bold;
    width:70px;
    min-width:70px;
}

.filtertable td{
    padding:0px;
}

.filterline{
    margin:10px;
    position:relative;
}

.filterline .filteroptions{
    line-height:18px;
    margin-left:80px;
    word-wrap: normal;
}

.filterline .filteroptions span{
    width:100px;
    height:18px;
    overflow:hidden;
    padding:2px 0;
    text-align:center;
    cursor:pointer;
    display: inline-block;
    border: 1px solid #6DA5DB;
    margin:1px 5px;
    background:#fff;
    vertical-align:middle;
}

.filterline .filteroptions .filterswitch{
    font-weight:bold;
    cursor:pointer;
}

.filterline .filteroptions .selected{
    color:white;
    background-color:#6DA5DB;
}

.filtertitlebar span{
    width:100px; 
    margin-right:10px;
    background:url(../../res/img/close.png) no-repeat right 2px #fff;
    display:none;
    cursor:pointer;
    border:1px solid #f80;
    padding:1px 20px 1px 3px;
}
.filtertitlebar{
    height:20px;padding:4px;
}
.optionswitch {
    position:absolute;
    right:0px;
    top:0px;
    background-color:#6DA5DB;
    color:white;
    padding-left:5px;
    padding-right:5px;
    cursor:pointer;
}
/*#type {
    display:none !important;
}*/
.ask-icon{
    background-image: url("../img/baidu_icon.png");
    background-position: -292px -238px;
    width: 19px;
    height: 19px;
    display: inline-block;
    left: 3px;
    position: relative;
    top: 6px;
}
/* 滑动侧边栏 */ 
#sideBar {
    position: fixed;
    width: auto;
    height: auto;
    top: 100px;
    right: 0px;
    background-image: url("../img/background.png");
    background-position: top left;
    background-repeat: repeat-y;
}
#sideBar{
    color:#fff;
    z-index:99999;
}

#sideBarTab {
    float: left;
    height: 137px;
    width: 28px;
}

#sideBarTab img {
    border: 0;
}

#sideBarContents {
    overflow: hidden !important;
}

#sideBarContentsInner {
    width: 225px;
    padding:15px;
}
