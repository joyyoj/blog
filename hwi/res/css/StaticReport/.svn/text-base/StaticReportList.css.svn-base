     a{                                                                         
         color:#0088CC;                                                         
         text-decoration:none;
         font-size:12px;
     }                                                                             
                                                                                   
     a:hover{                                                                      
       color:#005580;                                                              
       text-decoration:underline;                                                  
     }                                                                             
                                                                                   
     table.display{                                                                
         width:100%;                                                               
         margin:0px 0px;                                                           
         border-collapse:collapse;                                                 
         color:rgb(51,51,51);                                                      
     }                                                                             
                                                                                   
     table.display thead th {                                                      
         padding: 6px 12px 6px 8px;                                                
         border: 1px solid #AED0EA;                                                
         overflow:hidden;                                                          
         font-weight: bold;                                                        
         text-align:left;                                                          
         cursor: hand;                                                             
         color:#2779AA;                                                            
         background-color:#E3F1FA;                                                 
       }                                                                           
                                                                                   
     table.display td{                                                             
         padding: 10px !important;                                                 
         border: 1px solid #AED0EA;                                                
         overflow:hidden;                                                          
         font-size:14px;                                                           
     }                                                                             
                                                                                   
     table.display td a{                                                           
       font-weight:bold;                                                           
       color:#0088cc;
       font-size: 14px;
     }                                                                             
                                                                                   
     table.display td a:hover{                                                     
       color:#005580;                                                              
       text-decoration:underline;                                                  
     }
     tr.odd{                                                                       
       background-color:#ffffff;                                                   
     }                                                                             
                                                                                   
     tr.even{                                                                      
       background-color:#F5FBFE;                                                   
     }                                                                             
                                                                                   
     tr.over{                                                                      
       background-color:#f5f5f5;                                                   
     }                                                                             
                                                                                   
     .paginationarea{                                                              
       margin-top:10px;                                                            
       margin-bottom:20px;                                                      
     }                                                                          
                                                                                
     .paginationtip{                                                            
       float:left;                                                              
     }                                                                          
                                                                                
     .pagination{                                                               
       float:right;                                                             
     }                                                                          
                                                                                
     .pagination a{                                                             
       text-decoration:none !important;                                         
       backgound-color:#ddd;                                                    
       border:1px solid lightGrey;                                              
       padding:2px 5px;                                                         
       color:#333 !important;                                                   
       background:#E6E6E6;                                                      
       cursor:pointer;                                                          
     }                                                                          
                                                                                
    .paginate_button_disabled{                                                  
      opacity:0.35;                                                             
      filter:Alpha(Opacity=35);                                                 
      cursor:hand;                                                              
    }                                                                           
                                                                                
    .paginate_active{                                                           
      border:1px solid lightGrey;                                               
      padding:2px 5px;                                                          
      background:#E6E6E6;                                                       
      color:#333;                                                               
    }
