/* 修正字体颜色  */
body table{
    color:#333;
}

span.fixed{                                                                 
    border:1px solid #ABADB3;                                               
    background-color:#DBDFE6;                                               
    display:inline-block;                                                   
    border-right:none;                                                      
    padding:0px 0px 0px 0px;                                                
    margin:0px 0px 0px 0px;                                                 
    line-height:28px;                                                       
    height:28px;                                                            
}                                                                           
                                                                                   
a{                                                                            
    color:#0088CC;                                                            
    text-decoration:none;
    font-size:12px;
}                                                                             
                                                                                   
a:hover{                                                                      
    color:#005580;                                                              
    text-decoration:underline;                                                  
}                                                                             
                                                                                   
#redirectDialog{}                                                              
a.redirect{                                                                   
    text-decoration:underline;                                                  
    cursor:pointer;                                                             
    color:#005580;                                                              
}
                                                                               
a.redirect:-webkit-any-link {                                                   
    color: -webkit-link;                                                        
    text-decoration: underline;                                                 
    cursor: auto;                                                               
}                                                                               
                                                                                   
.container-inner{                                                             
    margin:0px 30px;                                                            
}                                                                              
                                                                                   
td.head {                                                                     
    text-align:right;
}                                                                           
                                                                                   
table.number{                                                                 
    border-collapse:collapse;                                                  
    margin:10px 0px;                                                           
}                                                                             
                                                                                   
table.number tr td{                                                           
    padding:0px;                                                              
    height:30px;                                                              
    border:1px solid #AED0EA;                                                 
    font-weight:bold;                                                         
    text-align:left;                                                          
    color:#2779AA;                                                            
}                                                                             
                                                                                   
table.number tr td.head{                                                      
    background-color:#E3F1FA;                                                 
} 
                                                                            
table.number tr td.head tr td.head select{                                                      
    padding:0px;
    margin:0px;
    background:red;
    height:50px;
    width:400px;
}
                                                                                   
table.file{                                                                   
    border-collapse:collapse;                                                  
    margin:10px 0px;                                                           
}

table.file thead th{                                                       
    padding:6px 12px 6px 8px;                                              
    border:1px solid #AED0EA;                                              
    overflow:hidden;                                                       
    font-weight:bold;                                                      
    text-align:center;                                                     
    color:#2779AA;                                                         
    background-color:#E3F1FA;                                              
    width:80px;
}                                                                          
                                                                                
table.file tr td{                                                          
    padding:6px 12px 6px 8px;                                              
    border:1px solid #AED0EA;                                              
    overflow:hidden;                                                       
    font-weight:bold;                                                      
    text-align:center;                                                     
    color:#2779AA;                                                         
}

input[type="text"]{
    border:1px solid rgb(171, 173, 179);
    border-radius:0px;
    -webkit-border-radius:0px;
    -moz-border-radius:0px;
    box-shadow:none;
}

input#txtFileColEnName,
input#input#txtFileColCnName{
    width:208px;
    height:26px;
    padding:0px;
    margin:0 0 1px 0;
}

input#txtNumRowEnName{
    height:28px;
    width:212px;    
    padding:0px;
    margin:0 0 1px 0;
}

/*
a[method="saveFileCol"]{
    margin-top:10px;
}
*/

form div fieldset{
    display: block;
    webkit-margin-start: 2px;
    webkit-margin-end: 2px;
    webkit-padding-before: 0.35em;
    -webkit-padding-start: 0.75em;
    -webkit-padding-end: 0.75em;
    -webkit-padding-after: 0.625em;
    border: 2px groove threedface;
    min-width: -webkit-min-content;
}

form div fieldset legend{
    display: inline-block;
    width:120px;
    margin-bottom:5px;
    border: none;
}

form div fieldset legend label{
    cursor: pointer;
    display: inline;
    font-size: 15px;
    line-height: 18px;
    text-underline: none;
}
