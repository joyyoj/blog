.table{border-collapse:collapse;border: 1px solid #AED0EA;}                    
.table td{padding:10px !important; font-size:13px;border: 1px solid #AED0EA;border-collapse:collapse}
.table .title{background-color: #E3F1FA !important;color: #2779AA;font-weight: bold;}
.table tbody tr:hover td, .table tbody tr:hover th {background-color: #F5FBFE;}
.span6, .span4{border-bottom:1px solid #AED0EA;text-align:center;height:18px;}
.span1{width:70px;}                                                            
.span6{width:710px;}                                                           
.row{text-align:center;margin:0 auto;margin-top:18px;}                         
.tab-content{border:1px solid #AED0EA;border-top:0px;}                         
.nav-tabs{margin-bottom:0px;border-color:#AED0EA;}                             
.nav-tabs .active a{border:1px solid #AED0EA;border-bottom-color:transparent;}
.nav-tabs .active a:hover {border:1px solid #AED0EA;border-bottom-color:transparent;}
table.display tbody td:first-child, table.display thead th:first-child{border-left:0;}
table.display tbody td:last-child, table.display thead th:last-child{border-right:0;}
.tabtop{margin:10px 0 0 10px;}                                                 
.dataTables_filter{margin:10px 10px 9px 0;}                                    
.dataTables_filter input, .dataTables_filter label{margin-bottom:0;}
